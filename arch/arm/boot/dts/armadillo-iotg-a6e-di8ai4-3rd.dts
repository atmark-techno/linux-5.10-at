/dts-v1/;
/plugin/;

#include "imx6ull-pinfunc.h"
#include <dt-bindings/interrupt-controller/irq.h>

&{/} {
	aliases {
		/* Aliases are represented by a string to a full node path.
		 * For overlays, specifying &adc4 will incorrectly result in a path
		 * within a fragment, which is not fixed at apply time.
		 * Specify the full path of the merged node directly.
		 */
		iio3 = "/soc/bus@2100000/i2c@21f8000/adc@4B";
	};
};

&i2c4 {
	#address-cells = <1>;
	#size-cells = <0>;

	adc4: adc@4B {
		compatible = "ti,ads1115";
		reg = <0x4B>;
		label = "di8ai4-3";

		#address-cells = <1>;
		#size-cells = <0>;

		#io-channel-cells = <1>;

		interrupt-parent = <&gpio4>;
		interrupts = <28 IRQ_TYPE_LEVEL_LOW>;

		channel@4 {
			reg = <4>;
			ti,gain = <0>;
			ti,datarate = <4>;
		};

		channel@5 {
			reg = <5>;
			ti,gain = <0>;
			ti,datarate = <4>;
		};

		channel@6 {
			reg = <6>;
			ti,gain = <0>;
			ti,datarate = <4>;
		};

		channel@7 {
			reg = <7>;
			ti,gain = <0>;
			ti,datarate = <4>;
		};
	};

	pi4ioe5v9554_3: gpio@25 {
		compatible = "nxp,pca9554";
		reg = <0x25>;

		gpio-controller;
		#gpio-cells = <2>;
	};
};
